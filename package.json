{
  "name": "learn-islam",
  "description": "A platform to learn Islam",
  "version": "0.0.1",
  "author": "abumalick",
  "license": "Unlicense",
  "scripts": {
    "build": "gatsby build",
    "build:prod": "eval $(cat .env.prod) gatsby build",
    "clean": "rimraf public .cache node_modules/.cache",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --watch --config codegen.yml",
    "commit": "git-cz",
    "full-build": "yarn build-plugin; yarn build",
    "build-plugin": "cd plugins/gatsby-source-rest/; yarn; yarn build:prod; cd ../../",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "stylelint --syntax=scss ./src/**/*.scss",
    "lint:js": "eslint .",
    "no-precommit": "lint-staged",
    "preview": "npm run clean && npm run build && yarn serve",
    "serve": "http-server ./public -p 8000 -P http://localhost:8000",
    "start": "npm run start:prod",
    "start-dawa": "npm run start-dawa:prod",
    "start:dev": "eval $(cat .env.dev) gatsby develop",
    "start-dawa:dev": "eval $(cat .dawa.env.dev) gatsby develop",
    "start-dawa:prod": "eval $(cat .dawa.env.prod) gatsby develop",
    "start:prod": "eval $(cat .env.prod) gatsby develop",
    "test": "jest",
    "test:security": "nsp check",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project ."
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --ignore-pattern '!.eslintrc.js'",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "git add"
    ],
    "*.{md}": [
      "prettier --write",
      "git add",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@material/drawer": "^0.35.0",
    "@material/form-field": "^0.37.1",
    "@material/list": "^0.35.0",
    "@material/radio": "^0.35.0",
    "@material/react-button": "^0.15.0",
    "@material/react-card": "^0.15.0",
    "@material/react-checkbox": "^0.15.0",
    "@material/theme": "^0.35.0",
    "@material/toolbar": "^0.35.0",
    "@material/typography": "^0.35.0",
    "babel-eslint": "^10.0.3",
    "caniuse-lite": "^1.0.30000989",
    "classnames": "^2.2.6",
    "core-js": "^2.6.9",
    "emotion-theming": "^10.0.17",
    "gatsby": "^2.15.3",
    "gatsby-image": "^2.2.16",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-canonical-urls": "^2.1.6",
    "gatsby-plugin-catch-links": "^2.1.7",
    "gatsby-plugin-emotion": "^4.1.5",
    "gatsby-plugin-favicon": "^3.1.5",
    "gatsby-plugin-load-script": "^1.0.6",
    "gatsby-plugin-mailchimp": "^5.1.2",
    "gatsby-plugin-manifest": "^2.2.13",
    "gatsby-plugin-netlify": "^2.1.11",
