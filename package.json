    "gatsby-plugin-sass": "^2.1.26",
    "gatsby-plugin-sitemap": "^2.2.24",
    "gatsby-plugin-theme-ui": "^0.2.43",
    "gatsby-plugin-typescript": "^2.1.23",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.42",
    "gatsby-source-graphql": "^2.1.28",
    "gatsby-transformer-json": "^2.2.22",
    "localforage": "^1.7.3",
    "make-plural": "^6.0.1",
    "memoize-one": "^5.1.1",
    "node-sass": "^4.13.0",
