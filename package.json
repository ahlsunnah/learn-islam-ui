    "gatsby-plugin-postcss": "^2.1.6",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-sass": "^2.1.13",
    "gatsby-plugin-sitemap": "^2.2.10",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-source-filesystem": "^2.1.19",
    "gatsby-source-graphql": "^2.1.12",
    "gatsby-transformer-json": "^2.2.7",
    "localforage": "^1.7.3",
    "make-plural": "^5.0.0",
    "memoize-one": "^5.1.1",
    "node-sass": "^4.11.0",
    "postcss-browser-reporter": "^0.6.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-reporter": "^6.0.1",
